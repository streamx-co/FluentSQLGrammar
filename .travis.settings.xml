<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      https://maven.apache.org/xsd/settings-1.0.0.xsd">
					  
  <servers>
    <server>
      <id>packagecloud-travis</id>
      <password>${env.PACKAGECLOUD_TOKEN}</password>
    </server>
  </servers>
  
  <profiles>
    <profile>
      <id>travis</id>
	  <repositories>
		  <repository>
			<id>kostat-ex-tree</id>
			<url>https://packagecloud.io/kostat/ex-tree/maven2</url>
			<releases>
			  <enabled>true</enabled>
			</releases>
			<snapshots>
			  <enabled>true</enabled>
			</snapshots>
		  </repository>
		  <repository>
			<id>kostat-jpa-notations</id>
			<url>https://packagecloud.io/kostat/jpa-notations/maven2</url>
			<releases>
			  <enabled>true</enabled>
			</releases>
			<snapshots>
			  <enabled>true</enabled>
			</snapshots>
		  </repository>
	  </repositories>
	</profile>
  </profiles>
  <activeProfiles>
   <activeProfile>travis</activeProfile>
 </activeProfiles>
</settings>